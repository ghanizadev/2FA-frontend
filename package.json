{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@types/styled-components": "^5.1.4",
    "axios": "^0.21.0",
    "babel-plugin-styled-components": "^1.12.0",
    "next": "^10.0.0",
    "qrcode.react": "^1.0.0",
    "qs": "^6.9.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-qr-code": "^1.0.3",
    "react-redux": "^7.2.2",
    "socket.io-client": "^3.0.3",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@types/qrcode.react": "^1.0.1",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/react-redux": "^7.1.11",
    "typescript": "^4.1.2"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
}
